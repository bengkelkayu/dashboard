{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Dashboard untuk mengelola daftar tamu undangan pernikahan dengan sistem kategorisasi VVIP, VIP, dan Regular.",
  "main": "backend/src/server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/src/server.js",
    "dev": "nodemon backend/src/server.js",
    "migrate": "node backend/migrations/run-migrations.js",
    "seed": "psql $DATABASE_URL < backend/migrations/seed-sample-data.sql || psql -U $DB_USER -d $DB_NAME < backend/migrations/seed-sample-data.sql",
    "worker": "node backend/src/workers/thankYouWorker.js"
  },
  "keywords": ["wedding", "guest", "dashboard", "attendance"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
